{
    "compilerOptions": {
        "outDir": "./build",
        "target": "es5",
        "lib": ["dom", "es7", "dom.iterable", "scripthost"],
        "moduleResolution": "node",
        "module": "commonjs",
        "downlevelIteration": true,
        "declaration": true,
        "removeComments": false,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    }
}
