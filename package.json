    "url": "https://github.com/Bibliofile/Blockheads-MessageBot/issues"
  },
  "homepage": "https://github.com/Bibliofile/Blockheads-MessageBot#readme",
  "scripts": {
    "test": "./node_modules/.bin/mocha --watch-extensions ts --watch -r ts-node/register src/**/*.test.ts",
    "start:cloud": "node -r ts-node/register src/index-cli.ts",
    "start:mac": "node -r ts-node/register src/index-mac.ts",
    "build": "./node_modules/.bin/gulp build",
    "docs": "./node_modules/.bin/typedoc --options ./typedoc.json",
    "watch": "./node_modules/.bin/gulp watch"
  },
  "browser": {
    "./src/libraries/ajax.ts": "./src/libraries/ajax-browser.ts"
  },
  "dependencies": {
    "@types/node": "^7.0.8",
    "@types/request": "0.0.42",
    "node-localstorage": "^1.3.0",
    "request": "^2.81.0",
    "sha1": "^1.1.1",
    "simple-plist": "^0.2.1",
    "ts-node": "^2.1.0",
    "typescript": "^2.2.1"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^13.3.0",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "mocha": "^3.2.0",
    "tsify": "^3.0.1",
    "tslint": "^5.0.0",
    "typedoc": "^0.5.9"
  }
