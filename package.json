{
  "name": "blockheads-messagebot",
  "version": "6.0.0",
  "description": "A bot for the Blockheads Cloud Portal",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bibliofile/Blockheads-MessageBot.git"
  },
